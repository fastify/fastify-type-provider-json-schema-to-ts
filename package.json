    "@fastify/pre-commit": "^2.1.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "fastify": "^5.0.0",
    "fastify-tsconfig": "^2.0.0",
    "neostandard": "^0.12.0",
    "rimraf": "^6.0.1",
    "tsd": "^0.31.0",
    "typescript": "~5.7.2"
